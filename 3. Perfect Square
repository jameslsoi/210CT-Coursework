'''
    FUNCTION pefect_square(number)
        
        square = sqrt (square)
        
        if square*square == number
            output <-- "This is a perfect square"
        else:
            square = square.floor
            result = square*square
            output <-- result
    
'''

import math
def perfect_Square(number):

    square = math.sqrt (number)

    if square*square == number:                                  
        print (" This is a perfect square")

    else:
        square = math.floor(square)
        result = square*square
        print (result)


perfect_Square(9)
perfect_Square(23)
perfect_Square(2)
