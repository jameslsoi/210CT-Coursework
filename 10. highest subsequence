def max_subseq(seq):

    final = []
    temp = [seq[0]]

    for item in range(len(seq) -1):
        first = seq[item]
        second = seq[item-1]

        if first > second:
            temp.append(first)
        elif len(temp) > len(final):
            final = temp
            temp = [first]

        else:
            temp = [first]


    print("Longest increasing sub sequence is: " + str(final))



#seq = [1,2,3,2,3,4,5,6,4,7]
#seq = [2,3,412,5,23,5,2,3,4]
#seq = [0,6,3,4,2,3,4,1,6,7,8,1,2,3,4]
max_subseq(seq)
