FUNCTION matrix_add (m1,m2) #big 0: 0(n^2)
    for entire row in m1
        for entire column m1
            m3[row, column] <-- m2[row,column] + m1[row,column]
    return m3
FUNCTION matrix_sub (m1,m2) #big 0 : 0(n^2)
    for entire row in m1
        for entire column in m1
            m3[row, column] <-- m2[row,column] - m1[row,column]
    return m3
FUNCTION matrix_multiplication (m1,m2)# big O : 0(n^3)
    for entire row in m1
        for entire column2 in m2
            for entire column1 in m1
                m3[row,column] <-- m1[row,column] * m2[column1,column2]
    return m3

# a = b*c - 2*(b+c)
a = matrix_multiplicati
