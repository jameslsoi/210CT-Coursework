def binary_Search (list, lower_bound, upper_bound):


    found = False


    if len(list) == 0:
        return False

    else:

        midpoint = len(list)//2
        if list[midpoint] > lower_bound and list[midpoint] < upper_bound:               #midpoint between upperbound and lowerbound

            return True

        elif list[midpoint] > lower_bound:
            return binary_Search(list[:midpoint],lower_bound, upper_bound)              #Recursive, list[0] --> midpoint[-1]

        else:
            return binary_Search(list[midpoint + 1:],lower_bound, upper_bound)         #Recursive, midpoint+1 to end of list

    return found


list = [2.5,3.5,5.5,7.5,11.5,13.5,17.5]
print (binary_Search(list, -2,3))
print (binary_Search(list, 14,16) )
